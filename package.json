  },
  "homepage": "https://github.com/ZacharyEggert/google-books-react-search#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongoose": "^5.13.1",
    "path": "^0.12.7",
    "session": "^0.1.0"
  },
  "engines": {
    "node": "14.16.0"
  }
}
